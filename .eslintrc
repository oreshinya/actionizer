{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "block-scoped-var": 2,
    "default-case": 2,
    "camelcase": [2, {"properties": "always"}],
    "dot-location": [2, "property"],
    "eqeqeq": 2,
    "no-alert": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-implied-eval": 2,
    "no-native-reassign": 2,
    "radix": [2, "always"],
    "yoda": 2,
    "max-depth": [2, 2],
    "max-len": [2, 80, 2],
    "max-nested-callbacks": [2, 2],
    "new-parens": 2,
    "no-nested-ternary": 2,
    "operator-linebreak": [2, "after"],
    "arrow-parens": [2, "always"],
    "arrow-spacing": 2,
    "prefer-arrow-callback": 2,
    "constructor-super": 2,
    "no-confusing-arrow": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-const": 2,
    "prefer-template": 2,
    "require-yield": 2
  }
}
